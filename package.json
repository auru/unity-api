{
  "name": "unity-api",
  "version": "1.0.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run test && npm run clean && ./node_modules/.bin/babel src --out-dir dist",
    "clean": "./node_modules/.bin/rimraf dist",
    "lint": "./node_modules/.bin/eslint --ignore-path=.gitignore --fix ./src",
    "lint-prod": "NODE_ENV='production' npm run lint",
    "prepublish": "npm run build",
    "precommit-msg": "echo 'Running pre-commit checks...' && exit 0",
    "test": "npm run lint-prod && ./node_modules/.bin/ava --verbose",
    "test-watch": "npm run lint && ./node_modules/.bin/ava --verbose --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auru/unity-api.git"
  },
  "author": "Vitaliy Blinovskov (blinovskov@yandex-team.ru)",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/auru/unity-api/issues"
  },
  "homepage": "https://github.com/auru/unity-api#readme",
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "unity-utils": "^1.0.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "^6.16.0",
    "babel-register": "^6.16.3",
    "eslint": "^3.7.1",
    "eslint-plugin-ava": "^3.1.0",
    "pre-commit": "^1.1.3",
    "rimraf": "^2.5.4"
  },
  "ava": {
    "files": [
      "test/**/*.spec.js"
    ],
    "source": [
      "src/*.js"
    ],
    "concurrency": 4,
    "failFast": false,
    "tap": false,
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  }
}
